/data/log/api2/log.log{
        daily
        copytruncate
        missingok
        rotate 365
        compress
        dateext
        size 1k
        sharedscripts
        postrotate
        touch /home/api2/hichao_backend/logreopen/logreopen_production
        endscript
}

